  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.0",
    "@types/react": "~18.2.14",
    "expo": "~49.0.10",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.4",
    "react-native-web": "~0.19.6",
    "typescript": "^5.1.3",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  }
}
