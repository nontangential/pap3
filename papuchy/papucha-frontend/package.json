{
  "name": "papucha-frontend",
  "version": "1.0.0",
  "main": "src/App.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "set-githooks": "mkdir -p ../../.git/hooks && cp githooks/* ../../.git/hooks/",
